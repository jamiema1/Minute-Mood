{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["plugin:react/recommended", "eslint:recommended"],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "indent": ["error", 2],
      "max-lines": ["error", 300],
      "max-len": [
        "error",
        80,
        {
          "ignorePattern": "^import\\s.+\\sfrom\\s.+$",
          "ignoreUrls": true
        }
      ],
      "react/prop-types": 0,
      "object-curly-spacing": "error",
      "react/no-unescaped-entities": 0,
      "semi": ["error", "never"]
    }
  }
  